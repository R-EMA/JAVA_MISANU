package paket;

public class Main {

	public static void main(String[] args) {
		// primena funkcionalnog interfejsa i lambda izraza
		
		NegativanBroj nb = (n) -> (n < 0);
		
		if(nb.isNegativan(5)) {
			System.out.println("Broj je negativan");
		}
		else {
			System.out.println("Broj je pozitivan");
		}

	}

}
