package demo;

public class B {
	/*
	 * II - članice klase
	 * 
	 * Klasa je šablon kojim se opisuju osobine i ponašanja 
	 * objekata iz stvarnog ili apstraktnog sveta
	 * 
	 * osobine su opisane atributima, a 
	 * ponašanje metodama
	 * 
	 */
	
	public String ime;
	
	public void info() {
		System.out.println("Hello, " + ime + "! It's nice to meet you!");
	}

}
