package demo;

public class B {
	/*
	 * II - članice klase
	 * 
	 * Klasa je šablon kojim se opisuju osobine i ponašanja 
	 * objekata iz stvarnog ili apstraktnog sveta
	 * 
	 * osobine su opisane atributima, a 
	 * ponašanje metodama
	 * 
	 */
	
	public String ime;
	
	public void info() {
		System.out.println("Zdravo, " + ime + "! Drago mi je da smo se upoznali. Ja sam metod demo klase B.");
	}

}
