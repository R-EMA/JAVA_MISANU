package paket;

public class NaprednaFor {

	public static void main(String[] args) {
		// foreach u JAVI,napredna for petlja
		int niz[] = {2,5,7,9,29};
		
		//Å¡tampanje standardnom for petljom
		System.out.println("Standardna for petlja: ");
		for(int i=0; i<niz.length; i++) 
			System.print(niz[i] + "  ");

	}

}
