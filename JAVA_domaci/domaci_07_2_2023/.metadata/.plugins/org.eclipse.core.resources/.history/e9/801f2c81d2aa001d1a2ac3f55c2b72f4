package teret;

/**
 * 
 * @author emma
 * 
 * Zadatak 3 - Klasa Sanduk, nasleÄ‘uje Teret
 * Sanduk je teret oblika kvadra zadatih duÅ¾ina ivica. Oznak vrste je S.
 *
 */

public class Sanduk extends Teret {

	private int a;
	private int b;
	private int c;
	
	public Sanduk(int tezina, int a, int b, int c) {
		super(tezina);
		super.vrstaTereta = 'S';
		this.a = a;
		this.b = b;
		this.c = c;
	}
	 
	@Override
	public int zapremina() {
		// zapremina sanduka
		return a*b*c;
	}
	

}
