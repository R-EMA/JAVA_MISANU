package predmeti;

/**
 * 
 * @author emma
 *
 * klasa koja čuva podatke o predmetima
 * tip predmeta, masa (težina) i njegov redni broj
 */
public class Cuvarkuca {
	
	char tip;
	int indeks;
	double masa;
	
	Cuvarkuca(char tip, int indeks, double masa) {
		this.tip = tip;
		this.indeks = indeks;
		this.masa = masa;
	}

}
