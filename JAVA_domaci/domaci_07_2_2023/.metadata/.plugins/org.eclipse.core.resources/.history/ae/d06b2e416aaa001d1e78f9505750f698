package teret;

/**
 * 
 * @author emma
 * 
 * Zadatak 3 - Klasa Sanduk, nasleÄ‘uje Teret
 * Sanduk je teret oblika kvadra zadatih duÅ¾ina ivica. Oznak vrste je S.
 *
 */

public class Sanduk extends Teret {

	private double a;
	private double b;
	private double c;
	//private int idTereta;
	
	public Sanduk(double a, double b, double c) {
		//super();
		//this.idTereta = 
		super.vrstaTereta = 'S';
		this.a = a;
		this.b = b;
		this.c = c;
	}
	 
	@Override
	public double zapremina() {
		// zapremina sanduka
		return a*b*c;
	}
	

}
